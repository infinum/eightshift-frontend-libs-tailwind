	"blockName": "video",
	"title": "Video",
	"description" : "Showcase a video from the Media library.",
	"category": "eightshift",
	"icon": {
		"src": "es-video"
	},
	"keywords": [
		"video",
		"picture",
		"img"
	],
	"components": {
